<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" onMouseDragged="#mouseDraggedHandler" onMousePressed="#mousePressedHandler" prefHeight="420.0" prefWidth="600.0" styleClass="root" xmlns:fx="http://javafx.com/fxml" fx:controller="cams.LoginFormController">
  <children>
    <ChoiceBox fx:id="choiceType" focusTraversable="false" layoutX="250.0" layoutY="152.0" prefHeight="30.0" prefWidth="200.0" styleClass="default-input" value="Stall">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="OFS Staff" />
          <String fx:value="Stall" />
        </FXCollections>
      </items>
    </ChoiceBox>
    <TextField fx:id="username" layoutX="250.0" layoutY="192.0" prefHeight="30.0" prefWidth="200.0" styleClass="default-input" />
    <PasswordField fx:id="password" layoutX="250.0" layoutY="232.0" prefHeight="30.0" prefWidth="200.0" styleClass="default-input" />
    <Button id="sign-in" fx:id="buttonSignIn" layoutX="250.0" layoutY="269.0" onAction="#login" prefHeight="30.0" prefWidth="100.0" text="sign in" />
    <Button id="exit" fx:id="buttonExit" layoutX="360.0" layoutY="269.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="90.0" text="exit" />
  </children>
  <stylesheets>
    <URL value="@LoginForm.css" />
  </stylesheets>
</AnchorPane>
